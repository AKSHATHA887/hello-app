{
    "devDependencies": {
        "@types/aws-lambda": "8.10.101",
        "@types/express": "4.17.13",
        "@types/jest": "28.1.6",
        "@types/node": "^18.0.6",
        "@types/passport": "1.0.9",
        "@types/swagger-jsdoc": "6.0.1",
        "@types/swagger-ui-express": "4.1.3",
        "@types/webpack": "5.28.0",
        "@typescript-eslint/eslint-plugin": "5.30.7",
        "@typescript-eslint/parser": "5.30.7",
        "archiver": "5.3.1",
        "eslint": "8.20.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.2.1",
        "fork-ts-checker-webpack-plugin": "7.2.13",
        "generate-package-json-webpack-plugin": "2.6.0",
        "jest": "28.1.3",
        "jest-junit": "14.0.0",
        "jest-standard-reporter": "2.0.0",
        "ncp": "2.0.0",
        "prettier": "2.7.1",
        "rimraf": "3.0.2",
        "secure-json-parse": "2.4.0",
        "ts-jest": "28.0.7",
        "ts-loader": "9.3.1",
        "ts-node": "10.9.1",
        "typescript": "4.7.4",
        "webpack": "5.73.0",
        "webpack-cli": "4.10.0",
        "webpack-node-externals": "3.0.0"
    },
    "name": "autoapi",
    "AIRID": "9403",
    "cloudProvider": "aws2",
    "version": "1.0.0",
    "description": "REBAR evergreen template for nodejs Serverless (AWS/Azure/GCP)",
    "main": "controllers/autoapi/autoapi.js",
    "scripts": {
        "start": "node dist/local.js",
        "start:local": "set ENVIRONMENT=local&& node dist/local.js",
        "test": "jest --collect-coverage",
        "test:local": "jest",
        "test:ci": "jest --collect-coverage",
        "test:integration": "jest --collect-coverage",
        "test:watch": " jest --watch",
        "build": "npx tsc -pretty -p .",
        "pwd": "dir",
        "lint": "npx eslint . --ext .ts",
        "lint:fix": "npx eslint . --ext .ts --fix",
        "prebuildpackage": "npx rimraf pkg_dist",
        "package": "npx webpack && cd .webpack && npm install",
        "postpackage": "node cio-bundler.js packagename=autoapi-9403-sls-pkg provider=aws2",
        "buildpackage": "npm run package",
        "emit-swagger": "npm run build && swagger-jsdoc -d dist/swaggerMetadata.json ./src/routes.ts -o openapi.json"
    },
    "dependencies": {
        "@google-cloud/bigquery": "^6.0.2",
        "@opentelemetry/api": "1.1.0",
        "@opentelemetry/instrumentation": "0.30.0",
        "@opentelemetry/instrumentation-express": "0.30.0",
        "@opentelemetry/instrumentation-http": "0.30.0",
        "@opentelemetry/node": "0.24.0",
        "@opentelemetry/tracing": "0.24.0",
        "cors": "2.8.5",
        "express": "^4.18.1",
        "morgan": "1.10.0",
        "odata-v4-sql": "^0.1.2",
        "passport": "0.6.0",
        "passport-azure-ad": "^4.3.4",
        "serverless-http": "3.0.1",
        "swagger-jsdoc": "6.2.1",
        "swagger-ui-express": "4.5.0"
    },
    "author": "REBAR App Arch",
    "schematics": {
        "@rebar/serverless:awsLambda": "1.0.0",
        "@rebar/serverless:logging": "0.3.51",
        "@rebar/serverless:headers": "0.3.51",
        "@rebar/serverless:errorhandler": "0.3.51",
        "@rebar/serverless:telemetry": "0.3.51",
        "@rebar/serverless:swagger": "0.3.51",
        "@rebar/serverless:rebar": "0.3.51",
        "@rebar/serverless:msal": "0.3.51"
    },
    "jest": {
        "globals": {
            "ts-jest": {
                "tsconfig": "tsconfig.json"
            }
        },
        "roots": [
            "./"
        ],
        "preset": "ts-jest",
        "testMatch": [
            "tests/*.ts",
            "tests/**/*.[t]s?(x)",
            "**/?(*.)+(spec|test).[t]s?(x)"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "dist/"
        ],
        "testEnvironment": "node",
        "collectCoverageFrom": [
            "src/**"
        ],
        "coverageReporters": [
            "json",
            "text",
            "cobertura",
            "html"
        ],
        "setupFiles": [],
        "reporters": [
            "jest-standard-reporter",
            [
                "jest-junit",
                {
                    "outputDirectory": "coverage",
                    "outputName": "junit.xml",
                    "classNameTemplate": "{classname}-{title}",
                    "titleTemplate": "{classname}-{title}",
                    "ancestorSeparator": " â€º ",
                    "usePathForSuiteName": "true"
                }
            ]
        ],
        "coveragePathIgnorePatterns": [
            "swaggerMetadata.json",
            "package.json",
            "package-lock.json",
            "node_modules",
            "config.json"
        ]
    },
    "eslintConfig": {
        "root": true,
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/eslint-recommended",
            "plugin:@typescript-eslint/recommended",
            "prettier"
        ],
        "env": {
            "jest": true,
            "node": true
        },
        "rules": {
            "prettier/prettier": [
                "error",
                {
                    "singleQuote": true,
                    "printWidth": 90
                }
            ],
            "complexity": [
                "error",
                10
            ]
        },
        "plugins": [
            "@typescript-eslint",
            "prettier"
        ]
    }
}